{
  "private": true,
  "version": "1.0.0",
  "description": "Cumulus Framework for ingesting and processing Nasa Earth data streams",
  "scripts": {
    "e2e": "ava tests/*.js --serial",
    "test": "lerna run test",
    "bootstrap": "lerna bootstrap",
    "ybootstrap": "lerna bootstrap --npm-client=yarn",
    "bootstrap-no-build": "lerna bootstrap --npm-client=yarn --ignore-scripts",
    "update": "lerna publish --skip-git --skip-npm",
    "publish": "lerna publish --skip-git --repo-version $VERSION --yes",
    "clean": "lerna clean",
    "build": "lerna run --parallel --no-sort build",
    "watch": "lerna run --parallel --no-sort watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cumulus-nasa/cumulus"
  },
  "ava": {
    "files": "test",
    "babel": "inherit",
    "require": [
      "babel-polyfill",
      "babel-register"
    ]
  },
  "keywords": [
    "GIBS",
    "CUMULUS",
    "NASA"
  ],
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-async-to-generator"
    ]
  },
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-core": "^6.13.2",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2017": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsdoc": "^3.3.1",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.0.0",
    "exports-loader": "^0.6.3",
    "json-loader": "^0.5.4",
    "lerna": "^2.9.0",
    "prepend-loader": "0.0.2",
    "source-map-support": "^0.4.2",
    "transform-loader": "^0.2.3",
    "webpack": "^1.13.3",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "fs-extra": "^5.0.0"
  }
}
