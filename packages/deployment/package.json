{
  "name": "@cumulus/deployment",
  "version": "1.3.0",
  "description": "Deployment templates for cumulus",
  "scripts": {
    "test": "ava --no-color",
    "build": "babel lib -d app",
    "watch": "babel lib -d app -w",
    "postinstall": "npm run build"
  },
  "keywords": [
    "CUMULUS"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cumulus-nasa/cumulus"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-polyfill",
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2017"
    ]
  },
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-cli": "^6.25.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2017": "^6.24.1",
    "extract-zip": "^1.6.6",
    "fs-extra": "^5.0.0",
    "kes": "~2.1.2",
    "lodash.get": "^4.4.2",
    "lodash.omit": "4.5.0",
    "node-forge": "^0.7.1",
    "p-limit": "^1.1.0",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^1.1.0",
    "@ava/babel-preset-transform-test-files": "^3.0.0",
    "ava": "^0.25.0"
  }
}
